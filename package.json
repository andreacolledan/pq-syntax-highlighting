{
  "name": "pq-syntax-highlighting",
  "displayName": "PQ Syntax Highlighting",
  "description": "Syntax highlighting for PQ",
  "repository" : "https://github.com/andcol/pq-syntax-highlighting",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pq",
        "aliases": [
          "PQ",
          "pq"
        ],
        "extensions": [
          ".pq"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pq",
        "scopeName": "source.pq",
        "path": "./syntaxes/pq.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
